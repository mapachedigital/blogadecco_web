        "js/select2.min.js",
        "css/select2.min.css",
        "js/i18n/es.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.21.0",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "additional-methods.min.js",
        "localization/messages_es.min.js"
      ]
    },
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/",
      "files": [
        "qrcode.min.js"
      ]
    },
    {
      "library": "bootstrap-icons@1.13.1",
